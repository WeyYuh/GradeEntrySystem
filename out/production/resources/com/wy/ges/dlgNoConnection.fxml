<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.wy.ges.DlgNoConnectionController"
            prefHeight="231" prefWidth="361">
    <children>
        <Label layoutX="33.0" layoutY="46.0" prefHeight="134.0" prefWidth="291.0" text="Check your internet connection and try again later. If problem persists, please contact administration" wrapText="true">
            <font>
                <Font name="Verdana" size="15.0" />
            </font>
        </Label>
        <Button fx:id="btnClose" cancelButton="true" layoutX="129.0" layoutY="166.0" mnemonicParsing="false" onAction="#btnCloseClick" prefHeight="27.0" prefWidth="101.0" style="-fx-background-color: #f07067;" text="Close" textFill="WHITE">
            <font>
                <Font name="Verdana Bold" size="13.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
